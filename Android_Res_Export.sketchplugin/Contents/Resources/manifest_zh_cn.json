{
    "name": "Android Res Export",
    "description": "导出 Android 资源, 包括 PNG 位图文件, 九宫格文件, 图标和可绘制矢量文件。",
    "author": "Ashung Hung",
    "email": "Ashung.hung@gmail.com",
    "homepage": "https://github.com/Ashung/Android_Res_Export",
    "manifestURL": "https://github.com/Ashung/Android_Res_Export/raw/master/Android_Res_Export.sketchplugin/Contents/Sketch/manifest.json",
    "downloadURL": "https://github.com/Ashung/Android_Res_Export/archive/master.zip",
    "version": "2.0.0",
    "identifier": "com.ashung.hung.android_res_export",
    "compatibleVersion": "3",
    "bundleVersion": "1",
    "commands": [
        {
            "name": "新建 PNG 位图资源",
            "identifier": "new_png_asset",
            "script": "new/new_png_asset.cocoascript"
        },
        {
            "name": "新建九宫格文件资源",
            "identifier": "new_nine_patch_asset",
            "script": "new/new_nine_patch_asset.cocoascript"
        },
        {
            "name": "新建可绘制矢量资源",
            "identifier": "new_vector_drawable_asset",
            "script": "new/new_vector_drawable_asset.cocoascript"
        },
        {
            "name": "新建应用图标",
            "identifier": "new_app_icon",
            "script": "new/new_app_icon.cocoascript"
        },
        {
            "name": "新建系统图标",
            "identifier": "new_system_icon",
            "script": "new/new_system_icon.cocoascript"
        },
        {
            "name": "导出资源",
            "identifier": "export_assets",
            "script": "export/export_assets.cocoascript"
        },
        {
            "name": "导出可绘制矢量",
            "identifier": "export_vector_drawable",
            "script": "export/export_vector_drawable.cocoascript"
        },
        {
            "name": "导出九宫格文件",
            "identifier": "export_nine_patch",
            "script": "export/export_nine_patch.cocoascript"
        },
        {
            "name": "导出应用图标",
            "identifier": "export_app_icon",
            "script": "export/export_app_icon.cocoascript"
        },
        {
            "name": "资源命名校验",
            "identifier": "asset_name_validator",
            "script": "export/asset_name_validator.cocoascript"
        },
        {
            "name": "网站",
            "identifier": "web_site",
            "handler": "website",
            "script": "Utilities/utilities.cocoascript"
        },
        {
            "name": "问题反馈",
            "identifier": "report_issues",
            "handler": "reportIssues",
            "script": "utilities/utilities.cocoascript"
        },
        {
            "name": "更改菜单语言",
            "identifier": "change_language",
            "script": "utilities/change_language.cocoascript"
        },
        {
            "name": "检测更新",
            "identifier": "update",
            "script": "utilities/update.cocoascript"
        },
        {
            "name": "通过 PayPal 打赏",
            "identifier": "donate_via_paypal",
            "handler": "donateViaPaypal",
            "script": "utilities/utilities.cocoascript"
        },
        {
            "name": "通过微信打赏",
            "identifier": "donate_via_wechat",
            "handler": "donateViaWechat",
            "script": "utilities/utilities.cocoascript"
        },
        {
            "name": "通过支付宝打赏",
            "identifier": "donate_via_alipay",
            "handler": "donateViaAlipay",
            "script": "utilities/utilities.cocoascript"
        }
    ],
    "menu": {
        "isRoot": false,
        "title": "Android Res Export",
        "items": [
            "new_app_icon",
            "new_system_icon",
            "-",
            "new_png_asset",
            "new_nine_patch_asset",
            "new_vector_drawable_asset",
            "-",
            "export_assets",
            "export_nine_patch",
            "export_vector_drawable",
            "export_app_icon",
            "-",
            "asset_name_validator",
            "-",
            {
                "title": "帮助",
                "items": [
                    "web_site",
                    "report_issues",
                    "-",
                    "update",
                    "-",
                    "change_language",
                    "-",
                    "donate_via_paypal",
                    "donate_via_wechat",
                    "donate_via_alipay"
                ]
            }
        ]
    }
}
