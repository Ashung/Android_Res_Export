{
    "name": "Android Res Export",
    "description": "用于导出 Android 各种资源的 Sketch 插件，包括位图资源、应用启动图标、点九（Nine-patch）图片、形状 XML 、色彩资源 XML 和矢量（Vector Drawable）文件。",
    "author": "Ashung Hung",
    "email": "Ashung.hung@gmail.com",
    "homepage": "https://github.com/Ashung/Android_Res_Export",
    "appcast": "https://ashung.github.io/Android_Res_Export/appcast.xml",
    "icon": "plugin_icon.png",
    "version": "2.3.18",
    "identifier": "com.ashung.hung.android_res_export",
    "compatibleVersion": "4",
    "bundleVersion": "1",
    "commands": [
        {
            "name": "新建位图资源",
            "identifier": "new_bitmap_asset",
            "handler": "newBitmapAsset",
            "script": "new/new_asset.cocoascript"
        },
        {
            "name": "新建矢量资源",
            "identifier": "new_vector_asset",
            "handler": "newVectorAsset",
            "script": "new/new_asset.cocoascript"
        },
        {
            "name": "新建点九资源",
            "identifier": "new_nine_patch_asset",
            "script": "new/new_nine_patch_asset.cocoascript"
        },
        {
            "name": "新建应用图标",
            "identifier": "new_app_icon",
            "script": "new/new_app_icon.cocoascript"
        },

        {
            "name": "点九图片预览",
            "identifier": "nine_patch_preview",
            "script": "preview/nine_patch_preview.cocoascript"
        },
        {
            "name": "从选中图层生成色彩资源代码",
            "identifier": "view_color_code_from_selected_layer",
            "script": "preview/view_color_code.cocoascript"
        },
        {
            "name": "查看形状资源代码",
            "identifier": "view_shape_code",
            "script": "preview/view_shape_code.cocoascript"
        },















        {
            "name": "导出位图资源",
            "identifier": "export_png_assets",
            "script": "export/export_png_assets.cocoascript"
        },
        {
            "name": "导出矢量资源",
            "identifier": "export_vector_drawable",
            "script": "export/export_vector_drawable.cocoascript"
        },
        {
            "name": "导出点九资源",
            "identifier": "export_nine_patch",
            "script": "export/export_nine_patch.cocoascript"
        },
        {
            "name": "导出应用图标",
            "identifier": "export_app_icon",
            "script": "export/export_app_icon.cocoascript"
        },




        {
            "name": "参数设置",
            "identifier": "preferences",
            "script": "utilities/preferences.cocoascript"
        },
        {
            "name": "网站",
            "identifier": "web_site",
            "handler": "website",
            "script": "utilities/utilities.cocoascript"
        },
        {
            "name": "问题反馈",
            "identifier": "report_issues",
            "handler": "reportIssues",
            "script": "utilities/utilities.cocoascript"
        },
        {
            "name": "捐款",
            "identifier": "donate",
            "handler": "donate",
            "script": "utilities/utilities.cocoascript"
        },
        {
            "handlers": {
                "actions": {
                    "OpenDocument": "onOpenDocument"
                }
            },
            "script": "utilities/change_language.cocoascript"
        }
    ],
    "menu": {
        "isRoot": false,
        "title": "Android Res Export",
        "items": [
            "new_bitmap_asset",
            "new_vector_asset",
            "new_nine_patch_asset",
            "new_app_icon",
            "-",
            "export_png_assets",
            "export_nine_patch",
            "export_vector_drawable",
            "export_app_icon",
            "-",
            "nine_patch_preview",
            "view_shape_code",
            "view_color_code_from_selected_layer",
            "-",
            "preferences",
            {
                "title": "帮助",
                "items": [
                    "web_site",
                    "report_issues",
                    "-",
                    "donate"
                ]
            }
        ]
    }
}
